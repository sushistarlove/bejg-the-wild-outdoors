gin\">\n            <Login />\n          </Route>\n\n          <Route exact path =\"/createpost\">\n            <CreatePost />\n          </Route>\n\n          <Route exact path =\"/viewpost\">\n            <ViewPost />\n          </Route>\n\n         \n          \n\n\n        </section>\n\n        \n{/* divs and Route exact paths go here */}\n\n<div>Hello World</div>\n\n      </Router>\n    </ApolloProvider>\n  );\n}\n\n\nexport default App;\n","C:\\Users\\Jared\\Desktop\\Projects\\Project-3-TheWildOutdoors\\bejg-the-wild-outdoors\\client\\src\\Components\\Explore.js",["70","71"],"C:\\Users\\Jared\\Desktop\\Projects\\Project-3-TheWildOutdoors\\bejg-the-wild-outdoors\\client\\src\\Components\\Login.js",[],"C:\\Users\\Jared\\Desktop\\Projects\\Project-3-TheWildOutdoors\\bejg-the-wild-outdoors\\client\\src\\Components\\Signup.js",[],"C:\\Users\\Jared\\Desktop\\Projects\\Project-3-TheWildOutdoors\\bejg-the-wild-outdoors\\client\\src\\Components\\EditPost.js",[],"C:\\Users\\Jared\\Desktop\\Projects\\Project-3-TheWildOutdoors\\bejg-the-wild-outdoors\\client\\src\\Components\\Comment.js",[],"C:\\Users\\Jared\\Desktop\\Projects\\Project-3-TheWildOutdoors\\bejg-the-wild-outdoors\\client\\src\\Components\\ViewPost.js",["72","73","74"],"import React from 'react';\nimport { Link } from 'react-router-dom';\nimport { Redirect, useParams } from 'react-router-dom';\nimport { useQuery } from '@apollo/client';\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome';\nimport { library } from '@fortawesome/fontawesome-svg-core';\nimport { fab } from '@fortawesome/free-brands-svg-icons';\nimport { faHiking, faFish, faCampground, faBinoculars, faSnowboarding, faAnchor, faMountain } from '@fortawesome/free-solid-svg-icons';\nimport './ViewPost.css';\n\n\nlibrary.add(fab, faHiking, faFish, faCampground, faBinoculars, faSnowboarding, faAnchor, faMountain);\n\n// import comments? editPost?\n//import queries\n// import Auth\n\nconst ViewPost = () => {\n    // useParams() to retriev